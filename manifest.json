{
  "name": "Google Account Recovery System",
  "short_name": "Google Recovery",
  "description": "Sistem pemulihan akun Google tanpa proxy",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4285f4",
  "icons": [
    {
      "src": "https://www.google.com/favicon.ico",
      "sizes": "64x64 128x128",
      "type": "image/x-icon"
    }
  ],
  "permissions": [
    "cookies",
    "https://accounts.google.com/*",
    "https://www.google.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-inline' https://accounts.google.com; object-src 'self'"
}